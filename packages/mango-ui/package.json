    "eslint-plugin-react": "^7.20.0",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "lint-staged": "^10.0.9",
    "normalize.css": "^8.0.1",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "rfs": "^9.0.2",
    "ts-jest": "^25.2.1",
    "ts-loader": "^6.2.1",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "@types/draft-js": "^0.10.38",
