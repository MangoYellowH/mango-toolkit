  "license": "MIT",
  "devDependencies": {
    "@mango-solution/eslint-config": "^0.0.5",
    "@testing-library/jest-dom": "^5.3.0",
    "@testing-library/react": "^10.0.2",
    "@types/events": "^3.0.0",
    "@types/jest": "^25.1.4",
    "@typescript-eslint/eslint-plugin": "^3.4.0",
    "@typescript-eslint/parser": "^3.4.0",
    "css-loader": "^3.4.2",
    "eslint": "^6.8.0",
