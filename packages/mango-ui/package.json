{
  "name": "@mango-solution/mango-ui",
  "version": "0.0.47",
  "description": "React based on Functional Component",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rm -rf ./dist",
    "build:js": "tsc",
    "build:scss": "cd src && find . -name '*.scss' | cpio -updm ../dist && cd ..",
    "build": "npm run clean && npm run build:js && npm run build:scss",
    "prepublish": "npm run build",
    "lint": "tsc --noEmit && eslint . --ext .ts,.tsx --fix",
    "test": "jest --passWithNoTests",
    "test-watch": "jest --watchAll"
  },
  "author": "huangyuan3h <huangyuan3h@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MangoYellowH/mango-toolkit.git"
  },
  "homepage": "https://github.com/MangoYellowH/mango-toolkit/blob/master/packages/mango-ui/README.md",
  "license": "MIT",
  "devDependencies": {
    "@mango-solution/eslint-config": "^0.0.15",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "13.1.9",
    "@types/events": "^3.0.0",
    "@types/jest": "^26.0.3",
    "@types/react-dom": "^16.9.8",
    "css-loader": "^3.6.0",
    "husky": "^4.2.3",
    "jest": "^26.1.0",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "ts-jest": "^26.1.1",
    "ts-loader": "^7.0.5",
    "typescript": "^3.9.5"
  },
  "dependencies": {
    "@types/draft-js": "^0.11.4",
    "clsx": "^1.1.1",
    "draft-js": "^0.11.7",
    "normalize.css": "^8.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rfs": "^9.0.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "pretty-quick --staged",
      "npm run lint",
      "git add"
    ]
  },
